<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="eCommerce Test Suite" parallel="tests">
    <listeners>
        <listener class-name="testComponent.Listeners"/>
    </listeners>
    
    <test thread-count="1" name="Submit Order Test">
        <classes>
            <class name="ritabhSelenium.eCommerce.submitOrderTest"/>
        </classes>
    </test>
    
    <test thread-count="1" name="Error Validation Test">
        <classes>
            <class name="ritabhSelenium.eCommerce.errorValidations"/>
        </classes>
    </test>
</suite>